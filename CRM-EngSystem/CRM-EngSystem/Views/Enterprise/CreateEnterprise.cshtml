<div class="container mt-5">
    <h1>Створити нове підприємство</h1>
    <form action="CreateEnterprise" method="POST">
        <div class="mb-3">
            <label for="nameUA" class="form-label">Назва підприємства (UA)</label>
            <input type="text" class="form-control" id="nameUA" name="NameUA" required>
        </div>
        <div class="mb-3">
            <label for="nameEN" class="form-label">Назва підприємства (EN)</label>
            <input type="text" class="form-control" id="nameEN" name="NameEN" required>
        </div>
        <div class="mb-3">
            <label for="ownershipFormUA" class="form-label">Форма власності (UA)</label>
            <input type="text" class="form-control" id="ownershipFormUA" name="OwnershipFormUA">
        </div>
        <div class="mb-3">
            <label for="ownershipFormEN" class="form-label">Форма власності (EN)</label>
            <input type="text" class="form-control" id="ownershipFormEN" name="OwnershipFormEN">
        </div>
        <div class="mb-3">
            <label for="industryBranch" class="form-label">Сфера діяльності</label>
            <input type="text" class="form-control" id="industryBranch" name="IndustryBranch">
        </div>
        <div class="mb-3">
            <label for="franchise" class="form-label">Франшиза</label>
            <input type="text" class="form-control" id="franchise" name="Franchise">
        </div>
        <div class="mb-3">
            <label for="country" class="form-label">Країна</label>
            <input type="text" class="form-control" id="country" name="Country">
        </div>
        <div class="mb-3">
            <label for="city" class="form-label">Місто</label>
            <input type="text" class="form-control" id="city" name="City">
        </div>
        <div class="mb-3">
            <label for="region" class="form-label">Область</label>
            <input type="text" class="form-control" id="region" name="Region">
        </div>
        <div class="mb-3">
            <label for="street" class="form-label">Вулиця</label>
            <input type="text" class="form-control" id="street" name="Street">
        </div>
        <div class="mb-3">
            <label for="postalCode" class="form-label">Поштовий індекс</label>
            <input type="text" class="form-control" id="postalCode" name="PostalCode">
        </div>
        <button type="submit" class="btn btn-primary">Створити</button>
    </form>
</div>
